{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "variable-name": [
      true,
      "check-format",
      "allow-snake-case",
      "allow-pascal-case"
    ],
    "interface-over-type-literal": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "member-access": false,
    // "no-any": true,
    "interface-name": false
  }
}
